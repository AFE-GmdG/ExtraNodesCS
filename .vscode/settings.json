{
  "files.exclude": {
    ".mono": true,
    ".vscode": true,
    "**/.git": true,
    "**/*.import": true,
    "*.csproj": true,
    "*.sln": true,
    "*.godot": true,
  },

  "files.encoding": "utf8",
  "files.eol": "\n",

  "explorer.sortOrder": "default",
  "explorer.sortOrderLexicographicOptions": "unicode",

  "omnisharp.enableEditorConfigSupport": true,
  "omnisharp.loggingLevel": "error",
}
